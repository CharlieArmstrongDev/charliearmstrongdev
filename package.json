{
  "name": "charliearmstrongdev-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "seed": "tsx scripts/seed-redis.ts",
    "seed:health": "tsx scripts/seed-redis.ts health",
    "seed:seed": "tsx scripts/seed-redis.ts seed",
    "seed:clear": "tsx scripts/seed-redis.ts clear",
    "seed:reset": "tsx scripts/seed-redis.ts reset"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "latest",
    "turbo": "latest",
    "undici": "^7.13.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@10.11.1+sha256.211e9990148495c9fc30b7e58396f7eeda83d9243eb75407ea4f8650fb161f7c",
  "pnpm": {
    "overrides": {
      "esbuild": "^0.25.5",
      "undici": "^7.10.0",
      "glob": "^8.1.0",
      "rimraf": "^5.0.0",
      "abab": "^2.0.6"
    },
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@sentry/cli",
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
    "dotenv": "^16.5.0"
  }
}
